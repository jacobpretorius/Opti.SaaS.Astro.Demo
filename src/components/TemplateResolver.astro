---
// Import page types i.e CMS models
import StartPageLayout from '../layouts/StartPage.astro';
import StandardPageLayout from '../layouts/StandardPage.astro';
import ImageRightContentPage from '../layouts/ImageRightContentPage.astro';

const { pageContent } = Astro.props;
//console.log(pageContent.__typename, pageContent);
---

{
  pageContent.__typename === 'StartPage' && (
    <StartPageLayout pageContent={pageContent} />
  )
}

{
  pageContent.__typename === 'StandardPage' && (
    <StandardPageLayout pageContent={pageContent} />
  )
}

{
  pageContent.__typename === 'ImageRightContentPage' && (
    <ImageRightContentPage pageContent={pageContent} />
  )
}
